<h1>Listing user_team_roles</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Role</th>
      <th>Desc</th>
      <th>Open flag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_team_roles.each do |user_team_role| %>
      <tr>
        <td><%= user_team_role.team_id %></td>
        <td><%= user_team_role.role %></td>
        <td><%= user_team_role.desc %></td>
        <td><%= user_team_role.open_flag %></td>
        <td><%= link_to 'Show', user_team_role %></td>
        <td><%= link_to 'Edit', edit_user_team_role_path(user_team_role) %></td>
        <td><%= link_to 'Destroy', user_team_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User team role', new_user_team_role_path %>
