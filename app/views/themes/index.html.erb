<h1>Listing themes</h1>

<table>
  <thead>
    <tr>
      <th>Topic</th>
      <th>Reward</th>
      <th>Policy</th>
      <th>Type</th>
      <th>Privacy flag</th>
      <th>Start time</th>
      <th>End time</th>
      <th>User</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @themes.each do |theme| %>
      <tr>
        <td><%= theme.topic %></td>
        <td><%= theme.reward %></td>
        <td><%= theme.policy %></td>
        <td><%= theme.type %></td>
        <td><%= theme.privacy_flag %></td>
        <td><%= theme.start_time %></td>
        <td><%= theme.end_time %></td>
        <td><%= theme.user_id %></td>
        <td><%= theme.team_id %></td>
        <td><%= link_to 'Show', theme %></td>
        <td><%= link_to 'Edit', edit_theme_path(theme) %></td>
        <td><%= link_to 'Destroy', theme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Theme', new_theme_path %>
