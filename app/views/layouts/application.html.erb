<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= yield :header %>
</head>
<body>
  <div class="container">
    <div class="row">
    </div>
    <div class="row">
      <div id="content">
          <%= content_for?(:content) ? yield(:content) : yield %>
      </div>
    </div>
  </div>
</body>
</html>
